<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE codgen-config PUBLIC "-//Apache Software Foundation//DTD Tengen Configuration 1.0//EN" "com/bcs/codgen/resources/codgen-config_1_0.dtd">
<codgen-config>
	<project name="xingzhen" label="刑侦" outputEncoding="UTF-8" isDefault="true">
		<dbProvider class="com.bcs.codgen.service.impl.OracleProvider">
			<jdbcConfig>
				<driver>oracle.jdbc.driver.OracleDriver</driver>
				<url>jdbc:oracle:thin:@192.168.1.249:1521:gdalims</url>
				<user>qd_lims</user>
				<password>qd_lims</password>
			</jdbcConfig>
			<columnHandler class="com.bcs.codgen.service.impl.DataTypeConverterForJava"/>
			<columnHandler class="columnHandler.OracleColumnHandler"/>
			<!-- 不使用系统默认的空格分隔符（要使用则去掉下面这行配置，当然可以设置其他分隔符来覆盖默认的空格分隔符） -->
			<splitorForLabelFromComment></splitorForLabelFromComment>
		</dbProvider>
		<!-- 自定义的构建配置处理器，可以在配置信息初始化、获取数据模型前后、获取输出模型前后时进行定制处理 -->
		<buildConfigHandler class="buildConfigHandler.SampleBuildConfigHandler" />
			
		<dataModel name="templateDirectory">template/ORACLE</dataModel>
		<dataModel name="outputDirectory">/xhh/Project-Workspaces/idea/base_framework/Server</dataModel>
		<dataModel name="NamespaceProject">com.hisign.framework</dataModel>
		<!-- <dataModel name="NamespaceDomain" >${NamespaceProject}.${groupName}.entity</dataModel>
		<dataModel name="NamespaceParam" >${NamespaceProject}.${groupName}.param</dataModel>
		<dataModel name="NamespaceDao">${NamespaceProject}.${groupName}.dao</dataModel>
		<dataModel name="NamespaceService">${NamespaceProject}.${groupName}.service</dataModel>
		<dataModel name="NamespaceServiceImpl">${NamespaceProject}.${groupName}.service.impl</dataModel> -->
		<!-- 自定义变量 -->
		<dataModel name="NamespaceDomain" >${NamespaceProject}.entity</dataModel>
		<dataModel name="NamespaceParam" >${NamespaceProject}.param</dataModel>
		<dataModel name="NamespaceDao">${NamespaceProject}.mapper</dataModel>
		<dataModel name="NamespaceService">${NamespaceProject}.service</dataModel>
		<dataModel name="NamespaceServiceImpl">${NamespaceProject}.service.impl</dataModel>
		
		<dataModel name="bfun">com.hisign.bfun</dataModel>
		<dataModel name="Conditions">${bfun}.bmodel.Conditions</dataModel>
		<dataModel name="UpdateParams">${bfun}.bmodel.UpdateParams</dataModel>
		
		<dataModel name="api">server-xz/server-xz-api</dataModel>
		<dataModel name="service">server-xz</dataModel>
		
		<dataModel name="CommonModelDir">${NamespaceProject}.api.entity</dataModel>
		<dataModel name="MapperDir">${NamespaceProject}.mapper</dataModel>
		<dataModel name="ModelDir">${NamespaceProject}.api.entity</dataModel>
		<dataModel name="serviceDir">${NamespaceProject}.api.service</dataModel>
		<dataModel name="serviceImplDir">${NamespaceProject}.service.impl</dataModel>
		
		<output name="domain" type="file">
			/${api}/src/main/java/${ModelDir?replace(".","/")}/${moduleName?cap_first}.java
		</output>
		<output name="daoxml" type="file">
			/${service}/src/main/resources/mapper/${moduleName?cap_first}Mapper.xml
		</output>
		<output name="dao" type="file">
			/${service}/src/main/java/${MapperDir?replace(".","/")}/${moduleName?cap_first}Mapper.java
		</output>
		<output name="service" type="file">
			/${api}/src/main/java/${serviceDir?replace(".","/")}/${moduleName?cap_first}Service.java
		</output>
		<output name="serviceImpl" type="file">
			/${service}/src/main/java/${serviceImplDir?replace(".","/")}/${moduleName?cap_first}ServiceImpl.java
		</output>
		
	</project>
</codgen-config>